<manifest>
	<!-- TEXTURES -->
	<resource>		
		<type>texture</type>
		<base>Assets/Textures/</base>								
		<item>
			<id>splashscreen.png</id>
			<data>
				<path>splashscreen.png</path>
			</data>
		</item>							
	</resource>
	
	<!-- FONTS -->
	<resource>
		<type>font</type>
		<base>Assets/Fonts/</base>				
		<item>
			<id>default</id>
			<data>
				<path>arial.ttf</path>
			</data>
		</item>		
		<item>
			<id>Main</id>
			<data>
				<path>arial.ttf</path>
			</data>
		</item>
	</resource>
	
	<!-- FILES -->
	<resource>
		<type>file</type>
		<base>Assets/</base>				
		<item>
			<id>keys.cfg</id>
			<data>
				<path>keys.cfg.txt</path>
			</data>
		</item>
	</resource>
	
	
